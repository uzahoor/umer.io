- var exper = data.sections[2];
h2.sectionTitle.clearfix(id=exper.title.shortName)
    i.fa.pull-right(aria-hidden='true' class=exper.title.icon)
    |  #{ exper.title.name }
hr

.contaniner
    .row
        each exp in exper.exp
            .col-12
                .card.border-dark
                    .card-header.text-white.bg-dark
                        h4.card-title #{ exp.company } - #{ exp.position }
                        h6.card-title #{ exp.time } 
                    .card-block
                        ul
                            each point in exp.points
                                li #{ point }